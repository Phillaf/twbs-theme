{
    "name": "gintonicweb/twbs-theme",
    "description": "Twitter Bootstrap default theme for GintonicCMS",
    "type": "cakephp-plugin",
    "homepage": "http://gintoniccms.com",
    "license": "MIT",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0",
        "gintonicweb/requirejs": "dev-master",
        "friendsofcake/bootstrap-ui": "~0.3",
        "gintonicweb/websockets": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "cakephp/cakephp-codesniffer": "dev-master"
    },
    "suggest": {
    },
    "autoload": {
        "psr-4": {
            "TwbsTheme\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TwbsTheme\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
